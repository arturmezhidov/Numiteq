@model Numiteq.ViewModels.Layout.FooterViewModel

<footer class="footer">
    <div class="section footer__inner">
        <div class="container">
            <div class="footer__top">
                <div class="title title--upper">@Model.FooterTitle</div>
                <div class="text text--light">@Model.FooterContent</div>
                <div class="contact-us-button">
                    <a href="@Model.ButtonLink" class="btn btn--outline btn--circle">@Model.ButtonText</a>
                </div>
            </div>
            <hr />
            <div class="footer__bottom">
                <div class="row align-items-center">
                    <div class="col-lg-2">
                        <a asp-area="" asp-controller="Home" asp-action="Index">
                            <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="136" height="57" viewBox="0 0 136 57">
                                <image id="logo" width="136" height="57" xlink:href="data:img/png;base64," />
                            </svg>
                        </a>
                    </div>
                    <div class="col-lg-7 align-right">
                        @if (!string.IsNullOrEmpty(Model.Link1Title))
                        {

                            <a href="@Model.Link1Url" class="btn">@Model.Link1Title</a>

                        }
                        @if (!string.IsNullOrEmpty(Model.Link2Title))
                        {

                            <a href="@Model.Link2Url" class="btn">@Model.Link2Title</a>

                        }
                        @if (!string.IsNullOrEmpty(Model.Link3Title))
                        {

                            <a href="@Model.Link3Url" class="btn">@Model.Link3Title</a>

                        }
                        @if (!string.IsNullOrEmpty(Model.Link4Title))
                        {

                            <a href="@Model.Link4Url" class="btn">@Model.Link4Title</a>

                        }
                        @if (!string.IsNullOrEmpty(Model.Link5Title))
                        {

                            <a href="@Model.Link5Url" class="btn">@Model.Link5Title</a>

                        }
                        @if (!string.IsNullOrEmpty(Model.Link6Title))
                        {

                            <a href="@Model.Link6Url" class="btn btn--small">@Model.Link6Title</a>

                        }
                    </div>
                    <div class="col-lg-3 align-right">
                        <a class="btn btn--full btn--circle btn--small" asp-area="" asp-controller="facebook" asp-action="Index">
                            <svg class="btn__icon" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="16" height="16" viewBox="0 0 16 16">
                                <metadata>
                                    <?xpacket begin="﻿" id="W5M0MpCehiHzreSzNTczkc9d"?>
                                    <x:xmpmeta xmlns:x="adobe:ns:meta/" x:xmptk="Adobe XMP Core 5.6-c142 79.160924, 2017/07/13-01:06:39        ">
                                        <rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#">
                                            <rdf:Description rdf:about="" />
                                        </rdf:RDF>
                                    </x:xmpmeta>
                                    <?xpacket end="w"?>
                                </metadata>
                                <image width="16" height="16" xlink:href="data:img/png;base64," />
                            </svg>
                            Write on Facebook
                        </a>
                    </div>
                </div>
            </div>
        </div>
    </div>

